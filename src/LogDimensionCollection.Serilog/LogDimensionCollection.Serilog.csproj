  <ItemGroup>
    <ProjectReference Include="..\LogDimensionCollection.AspNetCore\LogDimensionCollection.AspNetCore.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Serilog.Extensions.Hosting" Version="4.1.2" />
  </ItemGroup>

</Project>
